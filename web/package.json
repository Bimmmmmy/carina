  "name": "carina",
  "private": true,
  "scripts": {
    "build": "yarn relay && yarn build:main && yarn build:login && yarn build:collective",
    "build:profiler": "yarn relay && concurrently \"yarn build:main:profiler\" \"yarn build:login:profiler\" \"yarn build:collective:profiler\"",
    "build-storybook": "build-storybook",
    "prebuild": "rimraf ./build && rimraf ../cmd/bot/app/web/*.js && rimraf ../cmd/bot/app/web/*.html && rimraf ../dist",
    "postbuild": "cross-env node ./internals/AfterBuild.js",
    "clean": "rimraf ./build && rimraf ../cmd/bot/app/web/*.js && rimraf ../cmd/bot/app/web/*.html && rimraf ../dist && rimraf ./node_modules",
    "cra": "yarn relay && react-scripts start",
    "eject": "react-scripts eject",
    "preinstall": "node ./internals/CheckYarn.js",
    "relay": "yarn relay-compiler --watchman false",
    "start": "yarn relay && concurrently \"yarn wp:main\" \"yarn wp:login\" \"yarn wp:collective\"",
    "wp:main": "webpack serve --hot --config webpack/webpack.main.config.js --progress",
    "wp:login": "webpack serve --hot --config webpack/webpack.login.config.js --progress",
    "wp:collective": "webpack serve --hot --config webpack/webpack.collective.config.js --progress",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/webpack.main.prod.config.js --progress",
    "build:login": "cross-env NODE_ENV=production webpack --config webpack/webpack.login.prod.config.js --progress",
    "build:collective": "cross-env NODE_ENV=production webpack --config webpack/webpack.collective.prod.config.js --progress",
    "build:main:profiler": "cross-env NODE_ENV=production webpack --config webpack/webpack.main.prod.config.js --profile --json > main.json",
    "build:login:profiler": "cross-env NODE_ENV=production webpack --config webpack/webpack.login.prod.config.js --profile --json > login.json",
    "build:collective:profiler": "cross-env NODE_ENV=production webpack --config webpack/webpack.collective.prod.config.js --profile --json > collective.json",
    "storybook": "start-storybook -p 6006",
    "test": "react-scripts test",
    "update-schema": "get-graphql-schema http://localhost:8080/api/graphql > ./schema/schema.graphql"
  },
  "version": "3.0.0"
}
